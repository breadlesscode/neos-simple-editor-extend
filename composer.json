{
    "name": "breadlesscode/neos-simple-editor-extend",
    "description": "The simlpe way to extend the Neos CMS editor",
    "type": "neos-plugin",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Marvin Kuhn",
            "email": "marvin@kuhnweblab.com",
            "homepage": "https://kuhnweblab.com",
            "role": "Developer"
        }
    ],
    "require": {
        "neos/neos-ui-compiled": "^4.0 || ^5.0 || ^7.0",
        "neos/neos": "~4.3 || ^5.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Breadlesscode\\SimpleEditorExtend\\": "Classes/"
        }
    },
    "archive": {
